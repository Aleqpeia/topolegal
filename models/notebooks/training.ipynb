    "        \"                \\n\",\n",
    "        \"            except Exception as e:\\n\",\n",
    "        \"                print(f\\\"âš ï¸ Training step failed: {e}\\\")\\n\",\n",
    "        \"                continue\\n\",\n",
    "        \"        \\n\",\n",
    "        \"        # Calculate metrics\\n\",\n",
    "        \"        accuracy = accuracy_score(all_labels, all_predictions)\\n\",\n",
